# GoszakupAI â€” Environment Variables
# Copy to .env and fill in your values

# API Token for goszakup.gov.kz (leave empty for mock data)
GOSZAKUP_TOKEN=

# API Key for this service (required in production)
API_KEY=1234

# Comma-separated list of allowed origins for CORS
CORS_ALLOWED_ORIGINS="http://127.0.0.1:8006,null"

# Force model retraining on startup (1/true/yes to enable)
FORCE_TRAIN=0

# Export CatBoost training data to JSON on training
EXPORT_TRAIN_DATA=1

# Database (for production)
# DB_PASSWORD=changeme
